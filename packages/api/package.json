{
  "name": "@blobscan/api",
  "version": "0.0.1",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "dev": "pnpm run init:bucket",
    "init:bucket": "pnpm with-env ts-node ./scripts/init-bucket.ts",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "type-check": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --",
    "with-env:test": "dotenv -e ../../.env.test --",
    "test": "pnpm with-env:test vitest",
    "test:ui": "pnpm with-env:test vitest --ui"
  },
  "dependencies": {
    "@blobscan/blob-propagator": "^0.0.1",
    "@blobscan/blob-storage-manager": "^0.0.1",
    "@blobscan/dayjs": "^0.0.1",
    "@blobscan/db": "^0.0.1",
    "@blobscan/open-telemetry": "^0.0.1",
    "@blobscan/zod": "^0.0.1",
    "@trpc/server": "^10.43.2",
    "jsonwebtoken": "^9.0.0",
    "superjson": "1.9.1",
    "trpc-openapi": "^1.2.0"
  },
  "devDependencies": {
    "@blobscan/eslint-config": "^0.0.1",
    "@blobscan/test": "^0.0.1",
    "@types/jsonwebtoken": "^9.0.2"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@blobscan/eslint-config/base"
    ]
  }
}
