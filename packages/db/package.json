{
  "name": "@blobscan/db",
  "version": "0.0.1",
  "private": true,
  "main": "./index.ts",
  "types": "./index.ts",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "type-check": "tsc --noEmit",
    "db:generate": "pnpm with-env prisma generate",
    "db:push": "pnpm with-env prisma db push --skip-generate",
    "db:migrate": "pnpm with-env prisma migrate dev",
    "db:seed": "pnpm with-env prisma db seed",
    "db:reset": "pnpm with-env prisma db push --force-reset",
    "dev": "pnpm with-env prisma studio --port 5556",
    "with-env": "dotenv -e ../../.env --"
  },
  "prisma": {
    "seed": "ts-node prisma/seed/main.ts"
  },
  "dependencies": {
    "@blobscan/dayjs": "^0.0.1",
    "@prisma/client": "^5.0.0"
  },
  "devDependencies": {
    "@blobscan/blob-storage-manager": "^0.0.1",
    "@blobscan/eslint-config": "^0.0.1",
    "@blobscan/open-telemetry": "^0.0.1",
    "@faker-js/faker": "^8.0.2",
    "js-sha256": "^0.9.0",
    "prisma": "^5.0.0"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@blobscan/eslint-config/base"
    ]
  },
  "peerDependencies": {
    "@blobscan/open-telemetry": "^0.0.1"
  }
}
