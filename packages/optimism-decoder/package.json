{
  "name": "@blobscan/optimism-decoder",
  "version": "0.1.3",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "type-check": "tsc --noEmit",
    "op:decode": "npx dotenv -e ../../.env npx ts-node ./src/index.ts",
    "op:generate_ecotone_txt": "docker run -ti --rm postgres:16 psql $POSTGRES_URI -c \"SELECT hash FROM transaction WHERE from_id = '0x6887246668a3b87f54deb3b94ba47a6f63f32985' AND block_number BETWEEN 19429617 AND 20277188 ORDER BY block_number;\" > optimism_ecotone.txt",
    "op:download": "npx dotenv -e ../../.env npx ts-node ./src/download.ts optimism_ecotone.txt",
    "op:load-decoded-data-to-db": "npx dotenv -e ../../.env npx ts-node ./src/load-decoded-data-to-db.ts ./src/blobs/tx",
    "op:generate-import-csv": "npx dotenv -e ../../.env npx ts-node ./src/generate-import-csv.ts ./src/blobs/tx",
    "with-env:test": "dotenv -e ../../.env.test --",
    "test": "pnpm with-env:test vitest",
    "test:ui": "pnpm with-env:test vitest --ui"
  },
  "dependencies": {
    "@blobscan/db": "workspace:*",
    "axios": "^1.7.2",
    "csv-writer": "^1.6.0",
    "rlp": "^3.0.0"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@blobscan/eslint-config/base"
    ]
  }
}
