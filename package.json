{
  "name": "blobscan-monorepo",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=v18.15.0"
  },
  "packageManager": "pnpm@8.6.12",
  "scripts": {
    "build": "turbo build",
    "clean": "git clean -xdf node_modules",
    "clean:workspaces": "turbo clean",
    "db:generate": "turbo db:generate",
    "db:push": "turbo db:push db:generate",
    "db:studio": "pnpm -F db dev",
    "dev": "turbo dev --parallel",
    "format": "prettier --write \"**/*.{js,cjs,mjs,ts,tsx,md,json}\" --ignore-path .gitignore",
    "lint": "turbo lint && manypkg check",
    "lint:fix": "turbo lint:fix && manypkg fix",
    "test:int": "turbo test:int",
    "test:watch": "turbo run test:watch",
    "type-check": "turbo type-check",
    "backfill:overall": "ts-node  scripts/backfill-all-overall-stats.ts",
    "backfill:daily": "ts-node scripts/backfill-all-daily-stats.ts",
    "update:daily": "ts-node scripts/upsert-daily-stats.ts",
    "update:overall": "ts-node scripts/upsert-overall-stats.ts",
    "delete:daily": "ts-node scripts/delete-all-daily-stats.ts",
    "delete:overall": "ts-node scripts/delete-all-overall-stats.ts"
  },
  "dependencies": {
    "@blobscan/dayjs": "^0.0.1",
    "@blobscan/db": "^0.0.1",
    "@ianvs/prettier-plugin-sort-imports": "^3.7.2",
    "@manypkg/cli": "^0.20.0",
    "@sentry/node": "^7.64.0",
    "@types/node": "^18.16.19",
    "@types/prettier": "^2.7.3",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/ui": "^0.34.1",
    "dotenv-cli": "^7.2.1",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "prisma": "^5.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "turbo": "^1.10.7",
    "typescript": "^4.9.5",
    "vitest": "^0.34.1",
    "vitest-mock-extended": "^1.2.0"
  }
}
