    "react-dom": "^18.2.0",
    "swagger-ui-react": "^4.15.5",
    "trpc-openapi": "^1.1.2",
    "uuid": "^9.0.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@blobscan/eslint-config": "^0.1.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/swagger-ui-react": "^4.11.0",
    "@types/uuid": "^9.0.0",
