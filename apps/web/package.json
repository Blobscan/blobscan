{
  "name": "@blobscan/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true pnpm with-env next build",
    "build": "pnpm with-env next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "print-banner": "pnpm with-env ts-node ./src/scripts/print-banner.ts",
    "dev": "pnpm print-banner && pnpm with-env next dev -p 3000",
    "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "lint:fix": "pnpm lint --fix",
    "start": "pnpm print-banner && pnpm with-env next start",
    "svg:format": "svgo -r ./src/icons ./public",
    "type-check": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env  -v BLOB_PROPAGATOR_ENABLED=false --"
  },
  "dependencies": {
    "@blobscan/api": "^0.1.0",
    "@blobscan/dayjs": "^0.0.2",
    "@blobscan/open-telemetry": "^0.0.2",
    "@fontsource/inter": "^4.5.15",
    "@fontsource/public-sans": "^4.5.12",
    "@headlessui/react": "^1.7.13",
    "@headlessui/tailwindcss": "^0.1.3",
    "@heroicons/react": "^2.0.17",
    "@react-spring/web": "^9.7.2",
    "@t3-oss/env-nextjs": "^0.6.1",
    "@tanstack/react-query": "^4.28.0",
    "@trpc/client": "^10.43.2",
    "@trpc/next": "^10.43.2",
    "@trpc/react-query": "^10.43.2",
    "@trpc/server": "^10.43.2",
    "@upstash/feedback": "^2.0.0",
    "@upstash/ratelimit": "^0.4.4",
    "@vercel/analytics": "^1.0.2",
    "@vercel/kv": "^0.2.3",
    "classnames": "^2.3.2",
    "echarts": "^5.4.2",
    "echarts-for-react": "^3.0.2",
    "ethers": "^5.7.2",
    "next": "^13.4.16",
    "next-themes": "^0.2.1",
    "pretty-bytes": "^6.1.1",
    "react": "18.2.0",
    "react-blockies": "^1.4.1",
    "react-dom": "18.2.0",
    "react-loading-skeleton": "^3.3.1",
    "superjson": "1.9.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/forms": "^0.5.3",
    "@types/echarts": "^4.9.18",
    "@types/node": "^18.16.19",
    "@types/react": "^18.0.33",
    "@types/react-blockies": "^1.4.1",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.3.1"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@blobscan/eslint-config/base",
      "@blobscan/eslint-config/nextjs",
      "@blobscan/eslint-config/react"
    ]
  },
  "nextBundleAnalysis": {
    "budget": 358400,
    "budgetPercentIncreaseRed": 20,
    "minimumChangeThreshold": 500,
    "showDetails": true
  }
}
