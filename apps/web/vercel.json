{
  "build": {
    "env": {
      "NEXT_PUBLIC_BLOBSCAN_RELEASE": "git describe --tags"
    }
  },
  "buildCommand": "cd ../../packages/db && prisma generate && cd ../../ && turbo run build --filter=web",
  "installCommand": "pnpm install --store=node_modules/.pnpm-store",
  "framework": "nextjs",
  "git": {
    "deploymentEnabled": {
      "main": false
    }
  }
}
