    "swagger-ui-express": "^4.6.2",
    "trpc-openapi": "^1.1.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/swagger-ui-express": "^4.1.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@blobscan/eslint-config/base"
    ]
