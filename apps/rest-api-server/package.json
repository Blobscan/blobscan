{
  "name": "@blobscan/rest-api-server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "git clean -xdf node_modules",
    "dev": "pnpm run init:bucket && pnpm with-env ts-node-dev --require ./api/telemetry.ts --respawn --transpile-only --exit-child ./api/index.ts",
    "start": "pnpm with-env ts-node --require ./api/telemetry.ts api/index.ts",
    "with-env": "dotenv -e ../../.env --",
    "init:bucket": "pnpm with-env ts-node ../../scripts/init-bucket.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "@blobscan/api": "^0.0.1",
    "@blobscan/zod": "^0.0.1",
    "@opentelemetry/api": "^1.4.1",
    "@opentelemetry/auto-instrumentations-node": "^0.38.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.41.1",
    "@opentelemetry/resources": "^1.15.1",
    "@opentelemetry/sdk-node": "^0.41.1",
    "@opentelemetry/sdk-trace-node": "^1.15.1",
    "@opentelemetry/semantic-conventions": "^1.15.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "prom-client": "^14.2.0",
    "swagger-ui-express": "^4.6.2",
    "trpc-openapi": "^1.1.2",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@blobscan/eslint-config": "^0.0.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/swagger-ui-express": "^4.1.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@blobscan/eslint-config/base"
    ]
  }
}
