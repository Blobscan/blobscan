{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": ["**/.env"],
  "pipeline": {
    "db:generate": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "push": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "build": {
      "dependsOn": ["^build", "^db:generate"],
      "outputs": [".next/**", "dist/**"]
    },
    "lint": {},
    "lint:fix": {},
    "svg:format": {},
    "start": {
      "dependsOn": ["^build"],
      "persistent": true,
      "cache": false
    },
    "test": {
      "inputs": ["test/**/*.test.ts"]
    },
    "test:ui": {
      "inputs": ["test/**/*.test.ts"]
    },
    "test:dev": {
      "inputs": ["test/**/*.test.ts"],
      "cache": false
    },
    "test:setup": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "type-check": {
      "dependsOn": ["@blobscan/db#db:generate"],
      "cache": false
    }
  },
  "globalEnv": [
    "BLOB_DATA_API_ENABLED",
    "BLOB_DATA_API_KEY",
    "NEXT_PUBLIC_BLOBSCAN_RELEASE",
    "PUBLIC_POSTHOG_ID",
    "PUBLIC_POSTHOG_HOST",
    "AUTH0_CLIENT_ID",
    "AUTH0_CLIENT_SECRET",
    "AUTH0_ISSUER",
    "ANALYZE",
    "BEACON_NODE_ENDPOINT",
    "BEE_ENDPOINT",
    "BLOB_PROPAGATOR_TMP_BLOB_STORAGE",
    "BLOBSCAN_API_BASE_URL",
    "BLOB_RECONCILER_CRON_PATTERN",
    "BLOB_RECONCILER_BATCH_SIZE",
    "BLOBSCAN_API_PORT",
    "CHAIN_ID",
    "SWARM_CHUNKSTORM_URL",
    "SWARM_CHUNKSTORM_ENABLED",
    "CI",
    "DATABASE_URL",
    "DIRECT_URL",
    "ETH_PRICE_SYNCER_ENABLED",
    "ETH_PRICE_SYNCER_CRON_PATTERN",
    "ETH_PRICE_SYNCER_CHAIN_ID",
    "ETH_PRICE_SYNCER_CHAIN_JSON_RPC_URL",
    "ETH_PRICE_SYNCER_ETH_USD_PRICE_FEED_CONTRACT_ADDRESS",
    "ETH_PRICE_SYNCER_TIME_TOLERANCE",
    "FEEDBACK_WEBHOOK_URL",
    "PRIMARY_BLOB_STORAGE",
    "GOOGLE_STORAGE_BUCKET_NAME",
    "GOOGLE_STORAGE_PROJECT_ID",
    "GOOGLE_SERVICE_KEY",
    "GOOGLE_STORAGE_API_ENDPOINT",
    "GOOGLE_STORAGE_ENABLED",
    "LOG_LEVEL",
    "PRISMA_BATCH_OPERATIONS_MAX_SIZE",
    "NEXTAUTH_URL",
    "NEXT_PUBLIC_VERCEL_ANALYTICS_ENABLED",
    "PUBLIC_NETWORK_NAME",
    "NETWORK_NAME",
    "PUBLIC_SUPPORTED_NETWORKS",
    "PUBLIC_BEACON_BASE_URL",
    "PUBLIC_EXPLORER_BASE_URL",
    "PUBLIC_SENTRY_DSN_WEB",
    "NEXT_BUILD_OUTPUT",
    "NEXT_PUBLIC_VERCEL_GIT_COMMIT_SHA",
    "NEXT_RUNTIME",
    "NODE_ENV",
    "METRICS_ENABLED",
    "TRACES_ENABLED",
    "OTLP_AUTH_USERNAME",
    "OTLP_AUTH_PASSWORD",
    "OTEL_DIAG_ENABLED",
    "OTEL_EXPORTER_OTLP_PROTOCOL",
    "OTEL_EXPORTER_OTLP_ENDPOINT",
    "POSTGRES_STORAGE_ENABLED",
    "REDIS_URI",
    "S3_STORAGE_BUCKET_NAME",
    "S3_STORAGE_ENDPOINT",
    "SECRET_KEY",
    "SENTRY_DSN_API",
    "SWARM_STAMP_CRON_PATTERN",
    "SKIP_ENV_VALIDATION",
    "SWARM_STORAGE_ENABLED",
    "SWARM_BATCH_ID",
    "VERCEL_URL",
    "VITEST_MAINNET_FORK_URL",
    "WEAVEVM_STORAGE_API_BASE_URL",
    "TS_NODE",
    "MODE",
    "LOG_LEVEL",
    "PUBLIC_MATOMO_TAG_MANAGER_CONTAINER_URL"
  ]
}
